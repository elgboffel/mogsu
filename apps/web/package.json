{
  "name": "@project/web",
  "description": "Web app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "pnpm prebuild && pnpm run build.types && pnpm run build.client && pnpm run build.server",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,mjs,cjs,json,graphql}\" --fix",
    "format": "prettier --write \"src/**/*.{css,scss,sass,ts,tsx,js,jsx,md,json,graphql,mjs,cjs}\"",
    "format.check": "prettier --check .",
    "preview": "vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "prepare": "panda codegen --silent",
    "prebuild": "pnpm prepare"
  },
  "dependencies": {
    "@project/cms-sdk": "workspace:*",
    "@project/builder-sdk": "workspace:*",
    "@project/css-preset": "workspace:*",
    "@project/ui": "workspace:*",
    "graphql-request": "6.1.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.10",
    "@builder.io/qwik-city": "1.2.10",
    "@builder.io/vite-plugin-macro": "~0.0.7",
    "@pandacss/dev": "0.11.1",
    "@types/eslint": "8.44.2",
    "@types/node": "20.5.1",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "common": "workspace:*",
    "eslint": "8.47.0",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-qwik": "1.2.7",
    "tsconfig": "workspace:*",
    "typescript": "5.1.6",
    "undici": "5.23.0",
    "vercel": "^29.1.1",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.0",
    "wrangler": "^3.0.0"
  }
}
