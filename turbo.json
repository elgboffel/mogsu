{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [
		"**/.env.*local"
	],
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build", "^prepare"
			],
			"outputs": [
				"dist/**",
				"lib/**",
				"lib-typings/**"
			]
		},
		"deploy": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
