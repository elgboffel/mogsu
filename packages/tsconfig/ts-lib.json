{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Typescript library",
	"extends": "./base.json",
	"compilerOptions": {
		"allowJs": true,
		"declaration": true,
		"declarationMap": true,
		"target": "ES2017",
		"module": "ES2022",
		"lib": ["es2022", "DOM", "DOM.Iterable"],
		"resolveJsonModule": true,
		"noEmit": true,
		"types": ["node"]
	}
}
