{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Qwik lib",
	"extends": "./base.json",
  "compilerOptions": {
		"allowJs": true,
		"target": "ES2017",
		"module": "ES2020",
		"lib": ["es2020", "DOM"],
		"jsx": "react-jsx",
		"jsxImportSource": "@builder.io/qwik",
		"strict": true,
		"declaration": true,
		"declarationMap": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"incremental": true,
		"isolatedModules": true
	}
}
